{
  "name": "NoteIt",
  "description": "Note && OCR",
  "version": "0.0.1",
  "manifest_version": 3,
  "action": {
    "default_title": "NoteIt",
    "default_popup": "popup.html"
  },
  "permissions": ["tabs", "activeTab", "scripting", "clipboardWrite"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentScript.js"]
    }
  ]
}
